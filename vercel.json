{
  "buildCommand": "npm run build",
  "outputDirectory": "public",
  "installCommand": "npm install",
  "routes": [
    {
      "src": "/src/(.*)",
      "status": 404
    },
    {
      "src": "/(.*\\.scss)",
      "status": 404
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
